{
    "kind": "InferenceService",
    "apiVersion": "serving.kubeflow.org/v1alpha2",
    "metadata": {
        "name": "iris-classifier",
        "namespace": "gitops",
        "creationTimestamp": null,
        "labels": {
            "fluentd": "true"
        }
    },
    "spec": {
        "default": {
            "predictor": {
                "sklearn": {
                    "storageUri": "gs://kfserving-samples/models/sklearn/iris",
                    "runtimeVersion": "v0.4.1",
                    "resources": {
                        "limits": {
                            "cpu": "1",
                            "memory": "2Gi"
                        },
                        "requests": {
                            "cpu": "1",
                            "memory": "2Gi"
                        }
                    }
                },
                "logger": {
                    "mode": "all"
                }
            }
        },
        "canary": {
            "predictor": {
                "xgboost": {
                    "storageUri": "gs://kfserving-samples/models/xgboost/iris",
                    "resources": {
                        "limits": {
                            "cpu": "1",
                            "memory": "1Gi",
                            "nvidia.com/gpu": "0"
                        },
                        "requests": {
                            "cpu": "100m",
                            "memory": "1Gi",
                            "nvidia.com/gpu": "0"
                        }
                    }
                },
                "logger": {
                    "mode": "all"
                }
            }
        },
        "canaryTrafficPercent": 10
    },
    "status": {}
}